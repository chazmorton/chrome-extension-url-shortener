{
    "name": "URL Shortener",
    "description": "Shorten URLs with Rebrandly API",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "/images/URL_transparent.png"
        }
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "/images/URL_transparent.png"
        }
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+F",
                "mac": "MacCtrl+Shift+F"
            },
            "description": "URL Shortener"
        }
    },
    "icons": {
        "128": "/images/URL_transparent.png"
    }
}